ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /home/wsy/miniconda3/envs/dgl-env/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /home/wsy/miniconda3/envs/dgl-env/include -fPIC -O2 -isystem /home/wsy/miniconda3/envs/dgl-env/include -fPIC -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include/TH -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/bear/workspace/singleGNN/src/sample/include -I/home/wsy/miniconda3/envs/dgl-env/include/python3.10 -c
post_cflags = -O3 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=sample_hop -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include/TH -I/home/wsy/miniconda3/envs/dgl-env/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda/include -I/home/bear/workspace/singleGNN/src/sample/include -I/home/wsy/miniconda3/envs/dgl-env/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=sample_hop -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_80,code=compute_80 -gencode=arch=compute_80,code=sm_80 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /home/bear/workspace/singleGNN/src/sample/build/temp.linux-x86_64-cpython-310/kernel/sample_node_kernel.o: cuda_compile /home/bear/workspace/singleGNN/src/sample/kernel/sample_node_kernel.cu
